    "@babel/preset-react": "^7.7.4",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-mobx": "^2.0.0",
    "css-loader": "^3.4.0",
