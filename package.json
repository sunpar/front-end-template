{
  "name": "clinical-trials-app",
  "version": "0.0.1",
  "description": "An app to explore clinical trials gov data",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js --env.NODE_ENV=production",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:cypress": "cypress open",
    "test:cypress:headless": "cypress run -b chromium --headless --config video=false",
    "format": "pretty-quick",
    "lint": "eslint --ext .js --ext .ts --ext .jsx --ext .tsx ./ ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roivant/rtm-front-end"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ]
  },
  "author": "Sundeep Paruchuri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunpar/rtm-front-end/issues"
  },
  "homepage": "https://github.com/roivant/rtm-front-end#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
